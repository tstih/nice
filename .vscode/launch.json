{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Windows Debugger",
            "type": "cppvsdbg",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/build/minimal.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build",
            "environment": [{ "name": "config", "value": "Debug" }],
            "console": "internalConsole",
            "symbolSearchPath": "${workspaceFolder}/build"
        }
    ]
}